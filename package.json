{
    "name": "cadmus-labs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "PUBLIC_URL=http://localhost:3001 next dev",
        "build": "next build && node generate-sitemap.js",
        "build:analyze": "ANALYZE=true next build",
        "start": "next start",
        "lint": "npm run lint:ts && npm run lint:css",
        "lint:ts": "next lint",
        "lint:css": "stylelint *.css **/*.css",
        "format": "prettier --write ."
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "next": "13.4.8",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^13.4.8",
        "@types/node": "20.4.0",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "eslint": "8.44.0",
        "eslint-config-next": "13.4.8",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^36.0.0",
        "next-pwa": "^5.6.0",
        "prettier": "^3.0.0",
        "sitemap": "^7.1.1",
        "stylelint": "^15.10.0",
        "stylelint-config-standard": "^34.0.0",
        "typescript": "5.1.6"
    }
}
